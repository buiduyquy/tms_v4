function set_item_shipment_from(value,name) {

    // change input value

    $('#shipment_from').val(name);

    $("#shipment_from").attr("data",value);

    $("#shipment_from").attr("code",'true');

    

    // hide proposition list

    $('#shipment_from_list_id').hide();

    $('#shipment_from').focus();



    var from = value;

    var to = $('#shipment_to').attr("data");

    var ngay = $('#shipment_date').val();

    if(to != ""){

        $.ajax({

            url: '<?php echo BASE_URL ?>/shipment/checkroad',

            type: 'POST',

            data: {road_from:from, road_to:to},

            success:function(data){



                if (data.trim() == 0) {



                    $('#shipment_charge').prop('disabled',true);

                    $('#shipment_ton').prop('disabled',true);

                }

                else{

                    $('#shipment_charge').prop('disabled',false);

                    $('#shipment_ton').prop('disabled',false);

                }

            }

        });



        $.ajax({

            url: '<?php echo BASE_URL ?>/shipment/getroute',

            type: 'POST',

            data: {road_from:from, road_to:to, ngay:ngay},

            success:function(data){
                var data = jQuery.parseJSON(data);

                $('#shipment_road_add').val(data.road_add);

                $('#shipment_road_add').val(function(index, value) {

                  return value

                    .replace(/\D/g, "")

                    .replace(/\B(?=(\d{3})+(?!\d))/g, ",")

                  ;

                });

                $('#route').html(data.data);

                $('#route').multiselect( 'refresh' );

                $('#route').multiselect({
                    click: function(event, ui){
                        var val = parseInt($('#shipment_road_add').val().replace(/\,/g, ""));
                        if(ui.checked == 1){
                            val = val + parseInt(ui.text);
                            $('#shipment_road_add').val(val);
                        }
                        else{
                            val = val - parseInt(ui.text);
                            $('#shipment_road_add').val(val);
                        }

                        $('#shipment_road_add').val(function(index, value) {

                          return value

                            .replace(/\D/g, "")

                            .replace(/\B(?=(\d{3})+(?!\d))/g, ",")

                          ;

                        });
                    },
                    checkAll: function(){
                        $('#shipment_road_add').val(data.road_add);

                        $('#shipment_road_add').val(function(index, value) {

                          return value

                            .replace(/\D/g, "")

                            .replace(/\B(?=(\d{3})+(?!\d))/g, ",")

                          ;

                        });
                    },
                    uncheckAll: function(){
                        $('#shipment_road_add').val(0);
                    }
                });

            }

        });

    }

}

